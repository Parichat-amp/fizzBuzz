var output = [];

function fizzBuzzGame() {
    
	while(number <= 100) {
		
		if (number % 15 === 0) {
	        output.push("FizzBuzz");
	    } 
	    else if (number % 3 === 0) {
	        output.push("Fizz");
	    } 
	    else if (number % 5 === 0) {
	        output.push("Buzz");
	    }
	    else {
	        output.push(number);
	    }
	
		number = output.length + 1;
	}
	
    console.log(output);
}
